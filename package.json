{
  "name": "laguna-extension",
  "version": "1.0.3",
  "description": "Multi-currency browser extension wallet supporting Laguna Chain",
  "main": "index.js",
  "scripts": {
    "lint": "cd src/popup && yarn lint",
    "watch": "npm-run-all --parallel watch:*",
    "watch:background": "NODE_ENV=development webpack --config webpack.config.js -w",
    "watch:popup": "npm run watch:build --prefix src/popup",
    "build:background": "NODE_ENV=production webpack --config webpack.config.js",
    "build:popup": "npm run build --prefix src/popup",
    "build": "npm run build:background && npm run build:popup",
    "clean": "rimraf dist && cpy manifest.json manifest-ff.json dist && cpy src/assets/* dist/assets",
    "prebuild": "npm run clean",
    "prewatch": "npm run clean",
    "setup": "yarn install && cd src/popup && yarn install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@types/chrome": "0.0.184",
    "@types/node": "^17.0.8",
    "@types/webextension-polyfill": "^0.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "cpy-cli": "^4.1.0",
    "css-loader": "^6.5.1",
    "mini-css-extract-plugin": "^2.5.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.4.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@polkadot/api": "^9.4.2",
    "@polkadot/extension-dapp": "^0.44.6",
    "@polkadot/extension-inject": "^0.44.6",
    "@polkadot/phishing": "^0.18.9",
    "@polkadot/ui-keyring": "^2.9.10",
    "@types/bcryptjs": "^2.4.2",
    "@types/react-router": "5.1.17",
    "@types/react-router-dom": "5.3.2",
    "@zxing/browser": "^0.1.1",
    "archiver": "^5.3.0",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "bcryptjs": "^2.4.3",
    "cpy": "^9.0.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "dotenv-webpack": "^7.1.0",
    "ethers": "^5.6.9",
    "@ethereumjs/tx": "4.0.0",
    "googleapis": "^100.0.0",
    "import-fresh": "^3.3.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "react-idle-timer": "^5.2.0",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "react-textarea-autosize": "^8.3.3",
    "sass": "^1.54.0",
